{
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/mod.ts"
        }
    ],
    "functions": {
        "**/*.[jt]s": {
            "runtime": "vercel-deno@0.6.1",
            "includeFiles": "**/*.json"
        }
    },
    "build": {
        "env": {
            "DENO_TSCONFIG": "tsconfig.json"
        }
    },
    "github": {
        "enabled": true
    }
}