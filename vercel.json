{
    "routes": [
        {
            "src": "/",
            "dest": "/api/index.ts"
        },
        {
            "src": "/search",
            "dest": "/api/index.ts"
        },
        {
            "src": "/query",
            "dest": "/api/index.ts"
        },
        {
            "src": "/annotations",
            "dest": "/api/index.ts"
        },
        {
            "src": "/tag-keys",
            "dest": "/api/index.ts"
        },
        {
            "src": "/tag-values",
            "dest": "/api/index.ts"
        }
    ],
    "functions": {
        "**/*.[jt]s": {
            "runtime": "vercel-deno@0.6.1",
            "includeFiles": "api/**/*.json"
        }
    },
    "build": {
        "env": {
            "DENO_TSCONFIG": "api/tsconfig.json"
        }
    }
}