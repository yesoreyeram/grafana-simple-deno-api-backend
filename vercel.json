{
    "routes": [
        {
            "src": "/*",
            "dest": "/api/index.ts"
        }
    ],
    "functions": {
        "**/*.[jt]s": {
            "runtime": "vercel-deno@0.6.1",
            "includeFiles": "api/**/*.json"
        }
    },
    "build": {
        "env": {
            "DENO_TSCONFIG": "api/tsconfig.json"
        }
    }
}